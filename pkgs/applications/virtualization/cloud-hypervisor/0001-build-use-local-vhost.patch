+++ b/Cargo.toml
@@ -54,6 +54,7 @@ vm-memory = "0.10.0"
 kvm-bindings = { git = "https://github.com/cloud-hypervisor/kvm-bindings", branch = "ch-v0.6.0-tdx" }
